<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="Euphratis" transaction-type="JTA">
		<jta-data-source>java:app/Euphratis</jta-data-source>
		<class>org.kniftosoft.entity.App</class>
		<class>org.kniftosoft.entity.Auftrag</class>
		<class>org.kniftosoft.entity.Log</class>
		<class>org.kniftosoft.entity.Maschine</class>
		<class>org.kniftosoft.entity.Produkt</class>
		<class>org.kniftosoft.entity.Session</class>
		<class>org.kniftosoft.entity.Subbedmaschine</class>
		<class>org.kniftosoft.entity.Subscribe</class>
		<class>org.kniftosoft.entity.User</class>
		<class>org.kniftosoft.entity.Useraccess</class>
		<class>org.kniftosoft.entity.Zustand</class>
		<properties>
			<property name="eclipselink.target-database" value="MySQL"/>
			<property name="eclipselink.profiler" value="QueryMonitor"/>
			<property name="eclipselink.jdbc.native-sql" value="true"/>
			<property name="javax.persistence.schema-generation.create-database-schemas" value="false"/>
			<property name="javax.persistence.schema-generation.database.action" value="create"/>
		</properties>
	</persistence-unit>
</persistence>
